<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=<device-width>, initial-scale=1.0">
    <title>Looking for Job</title>
    <style>
        td {
            padding: 5px;
        }
    </style>
</head>

<body>
    <h1>Looking for Job</h1>

    <table border="1">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Email</th>
                <th>Mobile</th>                
                <th>State</th>
                <th>District</th>
                <th>Education</th>
                <th>Comments</th>
                <th>Date</th>
            </tr>
        </thead>

        <tbody>
            <% for(var i=0; i < data.length; i++) { %>
                <tr>
                    <td><%= data[i].id %></td>
                    <td><%= data[i].name %></td>
                    <td><%= data[i].email %></td>
                    <td><%= data[i].mobile %></td>                   
                    <td><%= data[i].state %></td>
                    <td><%= data[i].district %></td>
                    <td><%= data[i].education %></td>
                    <td><%= data[i].comments %></td>
                    <td><%= data[i].dt %></td>                    
                </tr>
            <% } %>
        </tbody>
    </table>
</body>

</html>